<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>webapp.login</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>webapp.Register</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>displayanunt</servlet-name>
        <servlet-class>webapp.DisplayAnunt</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>displayanunt</servlet-name>
        <url-pattern>/displayanunt</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>sorteaza</servlet-name>
        <servlet-class>webapp.Sorteaza</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>sorteaza</servlet-name>
        <url-pattern>/sorteaza</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>prelucrareproces</servlet-name>
        <servlet-class>webapp.PrelucrareProces</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>prelucrareproces</servlet-name>
        <url-pattern>/prelucrareproces</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>anunturileMele</servlet-name>
        <servlet-class>webapp.AnunturileMele</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>anunturileMele</servlet-name>
        <url-pattern>/anunturileMele</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>modifica</servlet-name>
        <servlet-class>webapp.Modifica</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>modifica</servlet-name>
        <url-pattern>/modifica</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>adauga</servlet-name>
        <servlet-class>webapp.AdaugaAnunt</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>adauga</servlet-name>
        <url-pattern>/adauga</url-pattern>
    </servlet-mapping>
</web-app>